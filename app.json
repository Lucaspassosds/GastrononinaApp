{
  "expo": {
    "assetBundlePatterns": ["**/*"],
    "name": "GastrononinaApp",
    "slug": "GastrononinaApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/svg/nina-front.svg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#B1CB53"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.GastrononinaApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/svg/nina-front.svg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.GastrononinaApp"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-sqlite", "expo-font"]
  }
}
